{
	"name": "chatsemble",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "biome check --write src",
		"preview": "vite preview",
		"deploy": "wrangler deploy",
		"cf-types": "wrangler types",
		"db:generate": "drizzle-kit generate",
		"db:generate:chat": "DRIZZLE_SCHEMA=./src/server/durable-objects/chat-room/db/schema.ts DRIZZLE_OUT=./src/server/durable-objects/chat-room/db/migrations drizzle-kit generate --config=drizzle.do.config.ts",
		"db:generate:agent": "DRIZZLE_SCHEMA=./src/server/durable-objects/agent/db/schema.ts DRIZZLE_OUT=./src/server/durable-objects/agent/db/migrations drizzle-kit generate --config=drizzle.do.config.ts",
		"db:migrate:dev": "wrangler d1 migrations apply chatsemble-db --local",
		"db:migrate:prod": "wrangler d1 migrations apply chatsemble-db --remote",
		"db:seed:dev": "tsx src/server/db/seed.ts",
		"db:seed:dev:sql": "wrangler d1 execute chatsemble-db --file=src/server/db/seed.sql --local",
		"db:seed:prod:sql": "wrangler d1 execute chatsemble-db --file=src/server/db/seed.sql --remote",
		"db:studio": "drizzle-kit studio"
	},
	"dependencies": {
		"@ai-sdk/google": "^1.2.4",
		"@ai-sdk/openai": "^1.3.4",
		"@hono/zod-validator": "^0.4.3",
		"@hookform/resolvers": "^4.1.3",
		"@mendable/firecrawl-js": "^1.21.0",
		"@radix-ui/react-alert-dialog": "^1.1.6",
		"@radix-ui/react-avatar": "^1.1.3",
		"@radix-ui/react-checkbox": "^1.1.4",
		"@radix-ui/react-collapsible": "^1.1.3",
		"@radix-ui/react-dialog": "^1.1.6",
		"@radix-ui/react-dropdown-menu": "^2.1.6",
		"@radix-ui/react-label": "^2.1.2",
		"@radix-ui/react-popover": "^1.1.6",
		"@radix-ui/react-scroll-area": "^1.2.3",
		"@radix-ui/react-select": "^2.1.6",
		"@radix-ui/react-separator": "^1.1.2",
		"@radix-ui/react-slot": "^1.1.2",
		"@radix-ui/react-switch": "^1.1.3",
		"@radix-ui/react-tabs": "^1.1.3",
		"@radix-ui/react-toggle": "^1.1.2",
