		"@react-email/components": "^0.0.34",
		"@tailwindcss/vite": "^4.0.17",
		"@tanstack/react-query": "^5.70.0",
		"@tanstack/react-router": "^1.114.29",
		"@tiptap/extension-mention": "^2.11.6",
		"@tiptap/extension-placeholder": "^2.11.6",
		"@tiptap/pm": "^2.11.6",
		"@tiptap/react": "^2.11.6",
