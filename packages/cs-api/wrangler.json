{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "cs-api",
  "main": "src/index.ts",
  "compatibility_date": "2025-02-04",
  "migrations": [
    {
      "new_sqlite_classes": [
        "ChatDurableObject",
        "AgentDurableObject"
      ],
      "tag": "v1"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "CHAT_DURABLE_OBJECT",
        "class_name": "ChatDurableObject"
      },
      {
        "name": "AGENT_DURABLE_OBJECT",
        "class_name": "AgentDurableObject"
      }
    ]
  },
  "observability": {
    "enabled": true
  }, 
  "rules": [
    {
      "type": "Text",
      "globs": ["**/*.sql"],
      "fallthrough": true
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "cs-d1",
      "database_id": "90d6b36b-6705-40b3-b51a-7c49896ee6da"
    }
  ]
}
