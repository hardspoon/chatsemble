
{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "do-chat-app",
  "main": ".open-next/worker.js",
  "compatibility_date": "2024-09-26",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "minify": true,
  "assets": {
    "binding": "ASSETS",
    "directory": ".open-next/assets"
  },
  "observability": {
    "enabled": true
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "do-chat-d1",
      "database_id": "90d6b36b-6705-40b3-b51a-7c49896ee6da",
      "migrations_dir": "src/server/db/migrations"
    }
  ]
}
